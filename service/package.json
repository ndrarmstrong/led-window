{
  "name": "led-window-service",
  "description": "LED window service",
  "version": "0.0.0",
  "author": "Nicholas Armstrong @ndrarmstrong",
  "bin": {
    "ledwinctl": "./bin/run"
  },
  "bugs": "https://github.com/ndrarmstrong/led-window/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^3.0.1",
    "aedes": "^0.42.0",
    "cli-ux": "^5.4.6",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.5",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "globby": "^11.0.0",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ndrarmstrong/led-window",
  "license": "MIT",
  "main": "lib/index.js",
  "mocha": {
    "require": "ts-node/register",
    "watch-extensions": [
      "ts"
    ],
    "recursive": true,
    "reporter": "spec",
    "timeout": 5000
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ledwinctl",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "window": {
        "description": "Configure window modes"
      }
    }
  },
  "private": true,
  "repository": "ndrarmstrong/led-window",
  "scripts": {
    "lint": "eslint . --ext .ts,.js --fix",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "npm run-script lint",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}